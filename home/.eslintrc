{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "globals": {
    "window": true,
    "require": true,
    "define": true,
    "React": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-undef": 2,
    "eqeqeq": 2,
    "dot-notation": 0,
    "brace-style" : [2, "1tbs", { "allowSingleLine": true }],
    "complexity" : [2, 12],
    "consistent-return" : 2,
    //"strict" : [2, "global"],
    "indent" : [2, 4],
    "key-spacing": 0,
    "max-depth" : [2, 4],
    "max-nested-callbacks" : [2, 3],
    "no-console": 2,
    "no-debugger" : 2,
    "no-loop-func" : 2,
    "no-return-assign" : 2,
    "no-shadow" : 2,
    "no-underscore-dangle" : 0,
    "no-use-before-define" : 2,
    "quotes" : [2, "single", "avoid-escape"],
    "no-multi-spaces": 0,
    "no-spaced-func": 0,
    "yoda": 0,
    "no-caller": 0,
    "no-empty": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "all"}],
    "no-cond-assign": 0
  }
}
